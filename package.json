{
  "name": "devfixedassets",
  "version": "1.0.0",
  "description": "An app for assets reference to be used world wide",
  "main": "server.js",
  "engines": {
    "node": "8.9.4"
  },
  "scripts": {
    "client-install": "npm install --prefix client ",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \" npm run server \" \" npm run client \"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "author": "Paul Fleischer-Djoleto, CISSP",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "cloudinary": "^1.11.0",
    "concurrently": "^3.6.1",
    "express": "^4.16.3",
    "gravatar": "^1.6.0",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.22.2",
    "mongoose": "^4.13.15",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-scripts": "^2.0.5",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "validator": "^10.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
